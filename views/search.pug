extends layout/main.pug
include mixins/videoBlock.pug

block content
    .search__header
        h3 searching for #{searchingBy}
    .search__videos
        if videos.length == 0
            h5 No Videos Found
        for item of videos
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile:item.videoFile,
                id:item.id
            })